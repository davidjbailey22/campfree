<h1>My bookings</h1>
<div class="cards">
  <% @bookings.each do |booking| %>
  <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://images.unsplash.com/photo-1591965958224-05c1bdf9f3ff?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1950&q=80)">
  <p><%= booking.tent.title %></p>
  <p><%= booking.checkin_date.strftime("%d %B") %> to <%= booking.checkout_date.strftime("%d %B") %></p>
  <%= link_to "Edit", edit_tent_booking_path(booking), class: "btn btn-warning" %>
  <p></p>
  <%= link_to "Remove", booking_path(booking), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-warning" %>
  </div>
  <% end %>
</div>