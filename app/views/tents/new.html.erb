  <h3>Enter your new tent below</h3>
  <div class="container">
    <h4>Tell us more about it!</h4>
      <%= simple_form_for @tent do |f| %>
      <%= f.input :title %>
      <%= f.input :location %>
      <%= f.input :price %>
      <%= f.input :description %>
      <%= f.submit class: "btn btn-primary"%>
      <% end %>
      </div>
  